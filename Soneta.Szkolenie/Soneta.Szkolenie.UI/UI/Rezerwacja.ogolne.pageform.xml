<?xml version="1.0"?>

<DataForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:xsd="http://www.w3.org/2001/XMLSchema"
          xmlns="http://www.enova.pl/schema/form.xsd"
          RightName="Page:OgólnePage"
          Priority="3"
          DataType="Soneta.Szkolenie.Rezerwacja,Soneta.Szkolenie">
  <Page Name="OgólnePage" CaptionHtml="Rejestracja" DataContext="{DataSource}">
    <Group>
      <Field EditValue="{NrRezerwacji}" Width="16"/>
      <Field EditValue="{Klient}" Width="40"/>
      <Field EditValue="{Lot}" Width="*"/>
      <Field EditValue="{Lot.Cena}" IsReadOnly="true" Width="*"/>
      <Field EditValue="{Maszyna}" Width="*"/>
      <Field EditValue="{CzyOplacona}" CaptionHtml="Czy zapłacona" Width="*"/>
    </Group>
  </Page>
</DataForm>

